[user]
    # Imported from ~/.gitconfig_local

[include]
    path = ~/.gitconfig_local

[init]
    defaultBranch = main

[core]
    excludesfile = ~/.gitignore_global

[color "status"]
    added = yellow
    changed = green
    untracked = cyan
    branch = magenta

[alias]
    b = branch
    co = checkout
    cm = commit --message
    s = status
    a = add

[push]
    autoSetupRemote = true

[pull]
    rebase = true

[fetch]
    prune = true
