Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519_github

Host *
  AddKeysToAgent yes
  UseKeychain yes
  ConnectTimeout 2
  ForwardAgent yes
  HashKnownHosts yes
  IdentityFile ~/.ssh/id_ed25519
  KeepAlive yes
  ServerAliveCountMax 6
  ServerAliveInterval 30
  StrictHostKeyChecking ask
