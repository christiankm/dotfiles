Host *
  AddKeysToAgent yes
  ConnectTimeout 2
  ForwardAgent yes
  HashKnownHosts yes
  IdentityFile ~/.ssh/id_ed25519
  KeepAlive yes
  UseKeychain yes
  ServerAliveCountMax 6
  ServerAliveInterval 30
  StrictHostKeyChecking ask
